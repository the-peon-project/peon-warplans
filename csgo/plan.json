{
    "metadata" : {
        "game_uid"       : "csgo",
        "server_name"   : "default",
        "version"       : "1.0.0",
        "description"   : "A PEON csgo game server.",
        "mode"          : "steamcmd",
        "hostname"      : "peon.steamcmd.csgo",
        "image"         : "umlatt/steamcmd"
    },
    "ports" : [
        [ { "PORT_GAME" : ["27015","tcp/udp"] }, { "PORT_RCON" : ["37015","tcp/udp"] } ]
    ],
    "environment" : {
        "STEAM_ID"       : "740",
        "SERVER_NAME"    : null,
        "STEAM_GSLT"     : null,
        "CSGO_GAME_TYPE" : "0",
        "CSGO_GAME_MODE" : "0",
        "CSGO_MAP_GROUP" : "mg_active",
        "CSGO_MAP"       : "de_dust2" 
