{
    "metadata" : {
        "game_uid"      : "ark",
        "warcamp"       : "default",
        "version"       : "1.0.0",
        "description"   : "A PEON ark game server.",
        "mode"          : "steamcmd",
        "hostname"      : "peon.warcamp.ark",
        "image"         : "umlatt/steamcmd"
    },
    "ports" : [
            [ { "PORT" : ["7777", "udp"] },{ "RAWPORT" : ["7778", "udp"] }, { "QUERYPORT" : ["27015", "udp"] },{ "RCON" : ["27020", "udp"] } ]
    ],
    "environment" : {
        "STEAM_ID"       : "376030",
        "SERVER_NAME"    : null,
        "ADMIN_PASSWORD" : null,
        "PASSWORD"       : null
    },
    "volumes" : {
        "~!/actions"       : "/actions",
        "~!/data"          : "/home/steam/steamcmd/data"                     ,
        "~!/config"        : "/home/steam/config"                            ,
        "~!/user"          : "/home/steam/temp"                                 
    }
}
